<Project Sdk="Microsoft.NET.Sdk">

  <Sdk Name="Aspire.AppHost.Sdk" />

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$(SAMPLES_TFM)-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsAspireHost>true</IsAspireHost>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspire.Hosting.AppHost" />
    <PackageReference Include="C3D.Extensions.Aspire.IISExpress" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AppConfigCore\AppConfigCore.csproj" />
    <ProjectReference Include="..\AppConfigFramework\AppConfigFramework.csproj" />
  </ItemGroup>

</Project>
