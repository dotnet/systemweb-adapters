<Project Sdk="Microsoft.NET.Sdk">

  <Sdk Name="Aspire.AppHost.Sdk" Version="9.0.0" />

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsAspireHost>true</IsAspireHost>
    <UserSecretsId>81517742-4dc5-497f-8b2e-dbff086fce1a</UserSecretsId>
    <ManagePackageVersionsCentrally>false</ManagePackageVersionsCentrally>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>
      $(NoWarn);
      CA1727;
      CA1848;
    </NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspire.Hosting.AppHost" Version="9.0.0" />
    <PackageReference Include="ThisAssembly.Project" Version="1.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\RemoteSession\RemoteSessionCore\RemoteSessionCore.csproj" />
    <ProjectReference Include="..\..\RemoteSession\RemoteSessionFramework\RemoteSessionFramework.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ProjectProperty Include="SolutionName" />
    <ProjectProperty Include="SolutionDir" />
  </ItemGroup>

</Project>
